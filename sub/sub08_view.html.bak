<!-- 헤더 스크립트 영역 -->
<script type="text/javascript">
$(function () {
    // case2
    $('a[href="#"]').click(function (e) {
        e.preventDefault();
    });

    $(".sub_nav_box>li>a").click(function (e) {
        var link = $(this);
        var item = link.parent(".nav_box_ob");

        if (item.hasClass("nav_active")) {
            item.removeClass("nav_active").children("a").removeClass("nav_active");
        } else {
            item.addClass("nav_active").children("a").addClass("nav_active");
        }

        if (item.children(".nav_box").length > 0) {
            var href = link.attr("href");
            link.attr("href", "#");
            setTimeout(function () {
                link.attr("href", href);
            }, 300);
            e.preventDefault();
        }
    })
    $(".sub_nav_box>li>a").each(function () {
        var link = $(this);
        if (link.get(0).href === location.href) {
            link.addClass("nav_active").parents("li").addClass("nav_active");
            return false;
        }
    });

    $(".nav_box_ob").mouseover(function () {
        $("#new_hd").addClass("hd_active");
        $(".sub_nav_box").stop().slideDown(200);
    });

    $("#new_hd").mouseleave(function () {
        $("#new_hd").removeClass("hd_active");
        $(".sub_nav_box").stop().slideUp(100);
    });

    $(".nav_box_ob").mouseover(function () {
        $("#new_hd").addClass("hd_active2");
        $(".sub_nav_box").stop().slideDown(200);
    });

    $("#new_hd").mouseleave(function () {
        $("#new_hd").removeClass("hd_active2");
        $(".sub_nav_box").stop().slideUp(100);
    });

    $(".nav_box_ob").mouseover(function () {
        $("#new_hd").addClass("hd_active3");
        $(".sub_nav_box").stop().slideDown(200);
    });

    $("#new_hd").mouseleave(function () {
        $("#new_hd").removeClass("hd_active3");
        $(".sub_nav_box").stop().slideUp(100);
    });

    $(".nav_box_ob").mouseover(function () {
        $("#new_hd").addClass("hd_active4");
        $(".sub_nav_box").stop().slideDown(200);
    });

    $("#new_hd").mouseleave(function () {
        $("#new_hd").removeClass("hd_active4");
        $(".sub_nav_box").stop().slideUp(100);
    });

    $(".m_nav_ob > a").click(function () {
        //slide up all the link lists
        $(".m_sub_nav").slideUp();
        $(".m_nav_ob > a").removeClass("m_nav_active");
        //slide down the link list below the h3 clicked - only if its closed
        if (!$(this).next().is(":visible")) {
            $(this).next().slideDown();
            $(this).addClass("m_nav_active");
        }
    });

    $(".m_bars").click(function () {
        $(".m_nav_wrap").addClass("m_active");
        $(".m_bg").fadeIn(300);
    });

    $(".close_btn").click(function () {
        $(".m_nav_wrap").removeClass("m_active");
        $(".m_bg").fadeOut(300);
    });

	$(window).on("scroll", function () {
        if ($(this).scrollTop() >= 100) {
            $("#new_hd").addClass("active");
        } else {
            $("#new_hd").removeClass("active");
        }
    });

    function scrollNav() {
        if ($(this).scrollTop() >= 100) {
            $("#new_hd").addClass("active");
        } else {
            $("#new_hd").removeClass("active");
        }
    }
    scrollNav();

});
</script>

