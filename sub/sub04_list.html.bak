<!-- 그래프 스크립트 -->
<script>
	$(document).ready(function(){
		barChart();
	
		$(window).resize(function(){
			barChart();
		});
	
		function barChart(){
			$('.bar-chart').find('.progress').each(function(){
				var itemProgress = $(this),
				itemProgressWidth = $(this).parent().width() * ($(this).data('persen') / 100);
				itemProgress.css('width', itemProgressWidth);
			});
		}
	});
	</script>

