.content_section_5_wrap .banner_slide_wrap { display: block; width: 563px; height: 206px; margin-top: 20px; position: relative; overflow: hidden; border-radius: 16px; }

.content_section_5_wrap .banner_slide_wrap .inner_wrap { display: flex; width: 100%; height: 100%; float: left; }

.content_section_5_wrap .banner_slide_wrap .inner_wrap > li { display: inline-block; width: 100%; height: 100%; float: left; }

.content_section_5_wrap .banner_slide_wrap .inner_wrap > li > a img { width: 100%; height: 100%; object-fit: cover; }

.content_section_5_wrap .banner_slide_wrap .control_btn_stop { display: flex; align-items: center; width: auto; height: auto; font-family: 'esamanru'; font-size: 12px; font-weight: 500; position: absolute; right: 22px; bottom: 22px; z-index: 10; color: #444; background-color: #fff; border-radius: 100px; padding: 10px; }

.content_section_5_wrap .banner_slide_wrap .control_btn_stop:after { content: ''; display: inline-block; width: 24px; height: 24px; background-image: url("../images/main/stop.png"); background-position: center; background-size: auto; background-repeat: no-repeat; }
