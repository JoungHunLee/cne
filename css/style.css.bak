/* 나눔스퀘어네오 */
@font-face {
    font-family: 'NanumSquareNeo-Variable';
    src: url('https://cdn.jsdelivr.net/gh/projectnoonnu/noonfonts_11-01@1.0/NanumSquareNeo-Variable.woff2') format('woff2');
    font-weight: normal;
    font-style: normal;
}

/* 나눔스퀘어 라운드 */
@font-face {
    font-family: 'NanumSquareRound';
    src: url('https://cdn.jsdelivr.net/gh/projectnoonnu/noonfonts_two@1.0/NanumSquareRound.woff') format('woff');
    font-weight: normal;
    font-style: normal;
}



/* footer 영역 */
#footer {width:100%; background:#5b5b5b; height:auto;}
.footer_content {overflow:hidden;}
.footer_content address {font-size:15px; line-height:26px; letter-spacing:-0.5px; color:#fff; padding:70px 0; float:left; font-family: 'NanumSquareNeo-Variable',sans-serif;}
.footer_content .footer_logo {float:right; padding:70px 0;}